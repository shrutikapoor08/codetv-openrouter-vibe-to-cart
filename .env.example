# API Keys - Copy this file to .env and add your actual API keys

# Mock Mode - Set to true to use mock responses without making API calls
# Useful for testing without consuming API credits
MOCK_MODE=false

# OpenRouter API Key - Required for the LLM (not needed in mock mode)
# Get your API key from: https://openrouter.ai/keys
OPENROUTER_API_KEY=

# Optional: Server port (defaults to 3001 if not specified)
PORT=3001

# API Base URL - Frontend will use this to call the backend
VITE_API_URL=http://localhost:3001/api/vibe